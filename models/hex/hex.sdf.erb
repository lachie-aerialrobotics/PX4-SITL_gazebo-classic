BASE_MASS = 12
BASE_EQUIV_WIDTH = 1.0
BASE_IXX  = 0.08333 * BASE_MASS * (2 * BASE_EQUIV_WIDTH**2)
BASE_IYY  = BASE_IXX
BASE_IZZ  = BASE_IXX

ROTOR_MASS = 0.01 * ROTOR_SCALE**3
ROTOR_IXX = 9.75e-7 * ROTOR_SCALE**5
ROTOR_IYY = 0.000273104 * ROTOR_SCALE**5 * 10
ROTOR_IZZ = ROTOR_IYY
