        <transparency>0</transparency>
        <cast_shadows>1</cast_shadows>
      </visual>
      <collision name='base_link_collision'>
        <pose>0.07 0 -0.1 0 0 0</pose>
        <geometry>
          <box>
            <size>0.4 0.4 <%= COM_Z_OFFSET * 2 %> </size>
          </box>
        </geometry>
        <surface>
          <contact>
            <ode>
              <min_depth>0.001</min_depth>
